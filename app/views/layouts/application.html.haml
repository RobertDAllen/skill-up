!!!
%html
%head
  %title Skill Up Application
  = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true 
  = javascript_include_tag 'application', 'data-turbolinks-track' => true 
  = csrf_meta_tags 
%body
  %nav.navbar.navbar-default
    .container
      .navbar-header
        = link_to "Scorekeeping Log", root_path, class: "navbar-brand"
      .nav.navbar-nav.navbar-right
        = link_to "New Entry", new_workout_path, class: "navbar navbar-link"


%div{:class => "well col-md-6 center container col-sm-offset-3" }

  = yield


